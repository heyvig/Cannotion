{
	"name": "Cannotion",
	"description": "Creating GUI for Cannotion",
	"version": "1.0",
	"manifest_version": 3,
	"options_page": "./public/options.html",
	"background": {
    "service_worker": "./src/background.js"
  	},
  	"permissions": ["storage", "activeTab", "scripting", "webNavigation"],
		"action": {
    "default_popup": "./public/popup.html",
    "default_icon": {
      "16": "./public/images/get_started16.png",
      "32": "./public/images/get_started32.png",
      "48": "./public/images/get_started48.png",
      "128": "./public/images/get_started128.png"
    }
  },
  "icons": {
    "16": "./public/images/get_started16.png",
    "32": "./public/images/get_started32.png",
    "48": "./public/images/get_started48.png",
    "128": "./public/images/get_started128.png"
  }
}